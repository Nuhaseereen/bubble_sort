#include <stdio.h>
int main()
{
   int n;
   scanf("%d",&n);
   int a[n];
   for(int i=0;i<n;i++){
       scanf("%d",&a[i]);
   }
   int swap=0;
   int j=0;
   while(j<n){ 
       int f=0;
       for(int k=0;k<n-1;k++){
           if(a[k]>a[k+1]){
               int t=a[k];
               a[k]=a[k+1];
               a[k+1]=t;
               f=1;
           }
       }
       swap++;
       if(f==0) break; 
       j++;
   }
      printf("\n%d",swap);
    return 0;
}
